{
  "name": "platform-frontend",
  "version": "0.0.0",
  "scripts": {
    "serve": "vite --mode $npm_config_mode",
    "serve:dev": "npm run serve --mode=development",
    "build": "vite build --mode $npm_config_mode",
    "preview": "vite preview --port 5050",
    "deploy:dev": "npm run build --mode=development && aws s3 sync dist/ s3://textile-dev.frontier.cool --delete && aws cloudfront create-invalidation --distribution-id E399Q6A2FXF69L --paths '/*' --profile=default",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:unit": "cypress open-ct",
    "test:unit:ci": "cypress run-ct --quiet --reporter spec",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@hennge/vue3-pagination": "^1.0.17",
    "@popperjs/core": "^2.11.2",
    "dayjs": "^1.10.7",
    "dom-to-image": "^2.6.0",
    "events": "^3.3.0",
    "jspdf": "^2.5.1",
    "nouislider": "^15.5.1",
    "overlayscrollbars": "^1.13.1",
    "qrcode.vue": "^3.3.3",
    "three": "^0.137.5",
    "uuid": "^8.3.2",
    "vue": "^3.2.31",
    "vue-i18n": "^9.2.0-beta.35",
    "vue-router": "^4.0.12",
    "vue-virtual-scroller": "^2.0.0-alpha.1",
    "vue3-carousel": "^0.1.36",
    "vue3-click-away": "^1.2.1",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.2",
    "@cypress/vue": "^3.1.0",
    "@intlify/vite-plugin-vue-i18n": "^3.2.2",
    "@rushstack/eslint-patch": "^1.1.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "autoprefixer": "^10.4.2",
    "cypress": "^9.4.1",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "postcss": "^8.4.8",
    "prettier": "^2.5.1",
    "pug": "^3.0.2",
    "sass": "^1.49.7",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.23",
    "vite": "^2.7.13",
    "vite-plugin-svg-sprite-component": "^1.0.10"
  }
}
