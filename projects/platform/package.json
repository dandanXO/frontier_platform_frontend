{
  "name": "@frontier/platform",
  "version": "0.0.0",
  "scripts": {
    "serve": "bash ./scripts/fetchEnvVar.sh $npm_config_mode && vite --host --mode $npm_config_mode",
    "serve:dev": "npm run serve --mode=development",
    "serve:feat": "npm run serve --mode=feat",
    "build": "bash ./scripts/fetchEnvVar.sh $npm_config_mode && vite build --mode $npm_config_mode",
    "build:dev": "npm run build --mode=development",
    "deploy": "npm run build --mode=$npm_config_mode && bash ./scripts/deploy.sh $npm_config_mode",
    "deploy:dev": "npm run deploy --mode=development",
    "windows:serve": "bash ./scripts/fetchEnvVar.sh %npm_config_mode% && vite --mode %npm_config_mode%",
    "windows:serve:dev": "npm run windows:serve --mode=development",
    "windows:build": "bash ./scripts/fetchEnvVar.sh %npm_config_mode% && vite build --mode %npm_config_mode%",
    "windows:build:dev": "npm run windows:build --mode=development",
    "windows:deploy": "npm run windows:build --mode=%npm_config_mode% && bash ./scripts/deploy.sh %npm_config_mode%",
    "windows:deploy:dev": "npm run windows:deploy --mode=development",
    "preview": "vite preview --port 8080 --mode=development",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../../.gitignore",
    "format": "prettier --write '**/*.{js,ts,vue,json}' --ignore-path ../../.gitignore",
    "cy:open": "cypress open",
    "cy:run": "cypress run --spec 'cypress/e2e/**/*.cy.ts'",
    "test": "vitest run",
    "test:e2e": "bash ./scripts/e2eTesting.sh $npm_config_mode",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@scena/ruler": "^0.20.0",
    "@techstark/opencv-js": "4.6.0-release.1",
    "@tiptap/extension-document": "2.0.0-beta.209",
    "@tiptap/extension-mention": "2.0.0-beta.209",
    "@tiptap/extension-paragraph": "2.0.0-beta.209",
    "@tiptap/extension-placeholder": "2.0.0-beta.209",
    "@tiptap/extension-text": "2.0.0-beta.209",
    "@tiptap/suggestion": "2.0.0-beta.209",
    "@tiptap/vue-3": "2.0.0-beta.209",
    "@types/uuid": "^9.0.2",
    "@vee-validate/zod": "^4.11.3",
    "@vueuse/core": "^9.11.0",
    "ag-grid-enterprise": "^31.0.0",
    "ag-grid-vue3": "^31.0.0",
    "bignumber.js": "^9.1.2",
    "decimal.js": "^10.4.3",
    "dom-to-image": "^2.6.0",
    "echarts": "^5.4.2",
    "exifreader": "^4.23.1",
    "fuse.js": "^7.0.0",
    "frontier-3d-viewer": "^0.0.85",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jszip": "^3.10.1",
    "konva": "^8.3.14",
    "pdfjs-dist": "^3.11.174",
    "pica": "^9.0.1",
    "pinia": "^2.0.35",
    "prosemirror-commands": "^1.5.0",
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-model": "^1.19.0",
    "prosemirror-schema-list": "^1.2.2",
    "prosemirror-state": "^1.4.2",
    "prosemirror-transform": "^1.7.1",
    "prosemirror-view": "^1.30.0",
    "qrcode": "^1.5.0",
    "tippy.js": "^6.3.7",
    "uuid": "^8.3.2",
    "vee-validate": "^4.11.8",
    "vue-echarts": "^6.5.5",
    "vue-gtag": "^2.0.1",
    "vue-konva": "^3.0.2",
    "vue-virtual-scroller": "^2.0.0-alpha.1",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.0.2",
    "xlsx": "^0.18.5",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/pica": "^9.0.1"
  },
  "peerDependencies": {
    "vue": "3.3.4"
  }
}
