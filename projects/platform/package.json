{
  "name": "@frontier/platform",
  "version": "0.0.0",
  "scripts": {
    "serve": "bash ./scripts/fetchEnvVar.sh $npm_config_mode && vite --mode $npm_config_mode",
    "serve:dev": "npm run serve --mode=development",
    "build": "bash ./scripts/fetchEnvVar.sh $npm_config_mode && vite build --mode $npm_config_mode",
    "build:dev": "npm run build --mode=development",
    "deploy": "npm run build --mode=$npm_config_mode && bash ./scripts/deploy.sh $npm_config_mode",
    "deploy:dev": "npm run deploy --mode=development",
    "preview": "vite preview --port 8080 --mode=development",
    "test:e2e": "npx playwright test",
    "test:show-report": "npx playwright show-report",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../../.gitignore",
    "format": "prettier --write '**/*.{js,ts,vue,json}' --ignore-path ../../.gitignore"
  },
  "dependencies": {
    "@techstark/opencv-js": "4.6.0-release.1",
    "@tiptap/extension-document": "2.0.0-beta.209",
    "@tiptap/extension-mention": "2.0.0-beta.209",
    "@tiptap/extension-paragraph": "2.0.0-beta.209",
    "@tiptap/extension-placeholder": "2.0.0-beta.209",
    "@tiptap/extension-text": "2.0.0-beta.209",
    "@tiptap/suggestion": "2.0.0-beta.209",
    "@tiptap/vue-3": "2.0.0-beta.209",
    "decimal.js": "^10.4.3",
    "dom-to-image": "^2.6.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "konva": "^8.3.14",
    "pica": "^9.0.1",
    "prosemirror-commands": "^1.5.0",
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-model": "^1.19.0",
    "prosemirror-schema-list": "^1.2.2",
    "prosemirror-state": "^1.4.2",
    "prosemirror-transform": "^1.7.1",
    "prosemirror-view": "^1.30.0",
    "qrcode": "^1.5.0",
    "tippy.js": "^6.3.7",
    "uuid": "^8.3.2",
    "vue-gtag": "^2.0.1",
    "vue-konva": "^3.0.2",
    "vue-virtual-scroller": "^2.0.0-alpha.1",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.25.0",
    "@types/pica": "^9.0.1",
    "start-server-and-test": "^1.14.0"
  },
  "peerDependencies": {
    "vue": "3.2.37"
  }
}
