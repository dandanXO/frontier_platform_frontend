{
  "name": "@frontier/heuritech",
  "version": "0.0.0",
  "scripts": {
    "dev": "bash ./scripts/fetchEnvVar.sh development && vite",
    "build": "bash ./scripts/fetchEnvVar.sh production && vite build",
    "deploy": "npm run build && bash ./scripts/deploy.sh",
    "preview": "vite preview --port 4173",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../../.gitignore",
    "format": "prettier --write '**/*.{js,ts,vue,json}' --ignore-path ../../.gitignore"
  },
  "dependencies": {
    "image-map-resizer": "^1.0.10"
  }
}
