{
  "name": "@frontier/3d-viewer",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "bash ./scripts/fetchEnvVar.sh development && vite --mode development",
    "preview": "vite preview --port 4173 --mode=development",
    "build": "vite build --mode=development",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../../.gitignore",
    "format": "prettier --write '**/*.{js,ts,vue,json}' --ignore-path ../../.gitignore"
  },
  "dependencies": {
    "@vueuse/core": "^9.11.0",
    "chroma-js": "^2.4.2",
    "debounce": "^1.2.1",
    "ramda": "^0.28.0",
    "rgbquant": "^1.1.2",
    "three": "^0.137.5"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.1.4",
    "@types/debounce": "^1.2.1",
    "@types/ramda": "^0.28.15",
    "@types/three": "^0.144.0",
    "npm-run-all": "^4.1.5"
  }
}
